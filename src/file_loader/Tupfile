include_rules

: foreach $(BIN_FILE_LOADER_CPP) |> !cx |> %B.o <obj>
: <obj> |                        \
          $(ROOT)/<lib_dmit_fmt> \
          $(ROOT)/<lib_dmit_com> \
          $(ROOT)/<lib_dmit_lex> \
          $(ROOT)/<lib_dmit_src> \
          $(ROOT)/<lib_dmit_prs> \
                                 |> !cl |> $(BIN_FILE_LOADER_OUT) $(ROOT)/<bin_file_loader>
