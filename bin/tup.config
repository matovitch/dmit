#######################
#                     #
# BUILD CONFIGURATION #
#                     #
#######################

# Use debug information
CONFIG_IS_DEBUG=y

# Optimization level (0 to 3)
CONFIG_O_LEVEL=2

# Use clang address sanitizer
CONFIG_IS_ASAN=n

# Use clang memory sanitizer
CONFIG_IS_MSAN=n

# Create static executables
CONFIG_IS_STATIC=y
